{
  "name": "react-typescript",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/basarat/react-typescript.git"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules package-lock.json && npm install",
    "build:check": "tsc -p .",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:webpack": "webpack -p",
    "build": "npm run build:check && npm run build:server && npm run build:webpack",
    "start:server": "ts-node-dev src/server/server.ts",
    "start:webpack": "webpack-dev-server -d --content-base ./public",
    "start": "concurrently \"npm run start:server\" \"npm run start:webpack\""
  },
  "dependencies": {
    "@types/express": "4.17.0",
    "@types/lowdb": "1.0.9",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "axios": "0.19.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "4.1.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "formstate": "1.3.0",
    "html-webpack-plugin": "3.2.0",
    "lowdb": "1.0.0",
    "mobx": "5.11.0",
    "mobx-react": "6.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "takeme": "0.11.1",
    "ts-loader": "6.0.4",
    "ts-node-dev": "1.0.0-pre.40",
    "typescript": "3.5.3",
    "typestyle": "2.0.3",
    "uuid": "3.3.2",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "^3.1.14"
  }
}
